- sitemap.resources.group_by {|p| p.data["category"] }.each do |category, pages|
	-if category
		.col-md-4
			a.thumbnail href="/faq/#{category}/"
				.caption.text-center
					h3.text-capitalize=data.categories[category].name
					p= data.categories[category].description
					p
						strong= "#{pages.count} Articles"
